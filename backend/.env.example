# Copy to .env and fill these in
NODE_ENV=development
PORT=4000

# PostgreSQL (docker-compose uses these defaults)
PGHOST=localhost
PGPORT=5432
PGUSER=postgres
PGPASSWORD=postgres
PGDATABASE=uber_dev

# JWT
JWT_SECRET=replace-with-a-long-random-string
JWT_EXPIRES_IN=1d

# CORS (comma-separated origins or * for all in dev)
CORS_ORIGINS=*
